olumn":8}},{"generated":{"line":49,"column":21},"source":"app.js","original":{"line":63,"column":25}},{"generated":{"line":50,"column":0},"source":"app.js","original":{"line":64,"column":5}},{"generated":{"line":51,"column":0},"source":"app.js","original":{"line":65,"column":1}},{"generated":{"line":51,"column":1},"source":"app.js","original":{"line":54,"column":0}},{"name":"window","generated":{"line":53,"column":0},"source":"app.js","original":{"line":67,"column":0}},{"generated":{"line":53,"column":6},"source":"app.js","original":{"line":67,"column":6}},{"name":"addEventListener","generated":{"line":53,"column":7},"source":"app.js","original":{"line":67,"column":7}},{"generated":{"line":53,"column":23},"source":"app.js","original":{"line":67,"column":0}},{"generated":{"line":53,"column":24},"source":"app.js","original":{"line":67,"column":24}},{"generated":{"line":53,"column":36},"source":"app.js","original":{"line":67,"column":0}},{"name":"router","generated":{"line":53,"column":38},"source":"app.js","original":{"line":67,"column":38}},{"generated":{"line":53,"column":44},"source":"app.js","original":{"line":67,"column":0}},{"name":"router","generated":{"line":54,"column":0},"source":"app.js","original":{"line":69,"column":0}},{"generated":{"line":54,"column":6},"source":"app.js","original":{"line":69,"column":6}}],"sources":{"app.js":"const ajax = new XMLHttpRequest();\nconst NEWS_URL = 'https://api.hnpwa.com/v0/news/1.json';\nconst CONTENT_URL = 'https://api.hnpwa.com/v0/item/@id.json';\nconst container = document.getElementById('root');\n\nconst store = {\n    currentPage : 1\n};\n\nconst getData = (url) => {\n    ajax.open('GET', url, false);\n    ajax.send();\n\n    return JSON.parse(ajax.response);\n}\n\nconst displayNewsFeed = () => {\n    const newsFeeds = getData(NEWS_URL);\n\n    //이와 같이 반복문을 사용해서 HTML을 문자열을 사용해서 구현할때는 배열을 활용하는게 기본적이다.\n    const newsList = [\"<ul>\"];\n    const maxPageNumber = newsFeeds.length / 10;\n    for (let i = (store.currentPage - 1) * 10 ; i < store.currentPage * 10 ; i++) {\n        newsList.push(`<li><a href = #/detail/${newsFeeds[i].id}>${newsFeeds[i].title} (${newsFeeds[i].comments_count})</a></li>`);\n    }\n    newsList.push(\"</ul>\");\n\n    newsList.push(`\n        <div>\n            <a href=\"#/page/${store.currentPage - 1 > 1 ? store.currentPage - 1 : 1}\">이전 페이지</a>\n            <a href=\"#/page/${store.currentPage + 1 > maxPageNumber ? maxPageNumber : store.currentPage + 1 }\">다음 페이지</a>\n        </div>\n    `);\n\n    container.innerHTML = newsList.join('');\n}\n\nconst displayNewsDetail = () => {\n    // 앵커태그의 해시가 변경되었을때 이벤트가 발생한다.\n    // 해시를 CONTENT_URL의 id란에 넣고 API를 호출해야함\n    // 해시를 주소에서 가져와야 하는데 주소 맨끝에 해시가 붙어있으니까 코드는 다음과 같다\n    const id = location.hash.substring(9);\n\n    const newsContent = getData(CONTENT_URL.replace('@id', id));\n\n    container.innerHTML = `\n        <h1>${newsContent.title}</h1>\n        <div>\n            <a href = \"#/page/${store.currentPage}\">목록으로</a>\n        </div>\n    `;\n}\n\nconst router = () => {\n    const routePath = location.hash;\n    if (routePath == '') {\n        // #만 있으면 그냥 빈 문자열 ''로 나온다.\n        displayNewsFeed();\n    } else if (routePath.indexOf('/page') >= 0) {\n        store.currentPage = Number(routePath.substring(7));\n        displayNewsFeed();\n    } else {\n        displayNewsDetail();\n    }\n}\n\nwindow.addEventListener('hashchange', router);\n\nrouter();\n\n"},"lineCount":null}},"error":null,"hash":"1f20e3cdf0de9037cda1a1f213d1653b","cacheData":{"env":{}}}